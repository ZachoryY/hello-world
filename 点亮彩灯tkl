#include <Adafruit_NeoPixel.h>

Adafruit_NeoPixel strip = Adafruit_NeoPixel(3, 6, NEO_GRB + NEO_KHZ800);

void setup()
{
  strip.begin();
  strip.show();
}

void loop()
{
  strip.setPixelColor(1-1, 0,153,0);
  strip.show();
  strip.setPixelColor(2-1, 255,0,0);
  strip.show();
  strip.setPixelColor(3-1, 51,102,255);
  strip.show();

}
